{
    "dependencies": [],
    "install": [
        {
            "question": "Is this computer a mac? (yes/no)",
            "variable": "{is_mac}",
            "answer": {
                "yes": [
                    "copy --su brightness /usr/local/bin/brightness",
                    "copy --su current-power /usr/local/bin/current-power",
                    "copy --su keyboard-brightness /usr/local/bin/keyboard-brightness",
                    "copy --su mute /usr/local/bin/mute",
                    "copy --su umute /usr/local/bin/umute",
                    "copy --su volume /usr/local/bin/volume"
                ],
                "no": []
            }
        }
    ]
}